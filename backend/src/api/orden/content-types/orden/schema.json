{
  "kind": "collectionType",
  "collectionName": "ordens",
  "info": {
    "singularName": "orden",
    "pluralName": "ordens",
    "displayName": "Orden"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "items": {
      "type": "json"
    },
    "buyer": {
      "type": "json"
    },
    "shipping": {
      "type": "json"
    },
    "total": {
      "type": "decimal"
    },
    "estado": {
      "type": "enumeration",
      "enum": [
        "pendiente",
        "pagado",
        "fallido"
      ],
      "default": "pendiente"
    },
    "payment_id": {
      "type": "string"
    },
    "payment_status": {
      "type": "string"
    },
    "fecha_pago": {
      "type": "datetime"
    },
    "cliente": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "ordens"
    }
  }
}
